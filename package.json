{
	"name": "@coltenkrauter/umbro-cdk",
	"version": "0.14.0",
	"private": true,
	"type": "module",
	"keywords": [
		"typescript",
		"umbro",
		"infrastructure",
		"cdk",
		"dynamodb"
	],
	"author": "@coltenkrauter",
	"license": "MIT",
	"scripts": {
		"bootstrap": "npm run cdk -- --app 'npm run node -- ./lib/app.ts' bootstrap",
		"build": "tsc",
		"cdk": "npm run node -- ./node_modules/aws-cdk/bin/cdk",
		"deploy": "npm run cdk -- deploy --app 'npm run node -- ./lib/app.ts' --require-approval never",
		"deploy-all": "npm run cdk -- deploy --app 'npm run node -- ./lib/app.ts' --require-approval never --all",
		"deploy-staged": "npm run node -- ./scripts/deploy-staged.ts",
		"diff": "npm run cdk -- --app 'npm run node -- ./lib/app.ts' diff",
		"fix": "npm run lint -- --fix",
		"lint": "npx eslint lib/**/*.ts",
		"node": "tsx",
		"synth": "npm run cdk -- --app 'npm run node -- ./lib/app.ts' synth",
		"test": "npm run lint",
		"update-vercel-env": "npm run node -- ./scripts/update-vercel-env.ts",
		"upgrade:all": "npx npm-check-updates -u && npm install",
		"verify-setup": "npm run node -- ./scripts/verify-setup.ts"
	},
	"dependencies": {
		"@aws-sdk/client-cloudformation": "^3.864.0",
		"@aws-sdk/client-dynamodb": "^3.864.0",
		"@krauters/structures": "^1.3.0",
		"@vercel/sdk": "^1.10.4"
	},
	"devDependencies": {
		"@types/node": "^24.2.1",
		"aws-cdk": "^2.1024.0",
		"aws-cdk-lib": "^2.211.0",
		"constructs": "^10.4.2",
		"tsx": "^4.20.4",
		"typescript": "^5.9.2"
	}
}
